<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:RoadCaptain.App.Shared.Dialogs.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="RoadCaptain.App.Shared.Dialogs.AboutRoadCaptainDialog"
        Title="About RoadCaptain"
        DataContext="{x:Type viewModels:AboutRoadCaptainViewModel}">
    <StackPanel>
        <TextBlock>RoadCaptain version </TextBlock><TextBlock Text="{Binding Path=Version}" />
    </StackPanel>
</Window>